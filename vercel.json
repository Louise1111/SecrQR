{
  "builds": [
    {
      "src": "backendSecQR/wsgi.py",
      "use": "@vercel/python",
      "config": { "maxLambdaSize": "15mb", "runtime": "python3.10.11" }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "backendSecQR/wsgi.py",
      "rewrites": [{ "source": "/(.*)", "destination": "/" }]
    }
  ]
}
